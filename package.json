{
  "name": "webstack-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env TAILWIND_MODE=watch next dev",
    "build": "cross-env TAILWIND_MODE=build next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "eslint components/**/*.tsx components/**/*.ts app/**/*.tsx cms/**/*.ts",
    "typecheck": "tsc",
    "hygen": "hygen",
    "prepare": "husky install",
    "make:modules": "hygen module new",
    "autogen:modules": "node utils/createModules.js",
    "autogen:types": "yarn graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local && sed -i \"s/export type Maybe<T> = T | null;/export type Maybe<T> = T;/g\" interfaces/graphql.ts",
    "autogen": "yarn autogen:modules && yarn autogen:types"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.10",
    "@mui/x-date-pickers": "^6.15.0",
    "autoprefixer": "^10.4.15",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.12.2",
    "hamburger-react": "^2.5.0",
    "html-react-parser": "^4.2.2",
    "jwt-decode": "^4.0.0",
    "next": "13.4.19",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.29",
    "react": "18.2.0",
    "react-animate-height": "^3.2.2",
    "react-dom": "18.2.0",
    "react-portal": "^4.2.2",
    "sass": "^1.66.1",
    "swiper": "^10.3.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "isomorphic-fetch": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
